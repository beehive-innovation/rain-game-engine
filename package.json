{
  "name": "rain1155",
  "version": "0.0.1",
  "description": "rain version of ERC1155 contract for decentralised games",
  "scripts": {
    "build": "hardhat compile",
    "test": "hardhat test",
    "docgen": "solidity-docgen -o ./docs/api/ --solc-module solc-0.8 --exclude ./contracts/test/ -t ./.templates/",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "codegen": "graph codegen",
    "build-graph": "graph build",
    "hardhat-node": "hardhat node",
    "copy-abis": "cp artifacts/contracts/Rain1155.sol/Rain1155.json abis/",
    "deploy:rain": "npm run prepare:mumbai && graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-game-engine-mumbai-v1",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/blocks",
    "create-local": "graph create --node http://localhost:8020/ vishalkale151071/blocks",
    "remove-local": "graph remove --node http://localhost:8020/ vishalkale151071/blocks",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 vishalkale151071/blocks --version-label 1",
    "m1-graph-node": "docker-compose -f \"graph-node/docker/docker-compose.yml\" up",
    "graph-node": "docker-compose -f \"docker/docker-compose.yml\" up",
    "prepare:mumbai": "npm run build && npx mustache config/mumbai.json subgraph.template.yaml subgraph.yaml && npm run copy-abis && npm run codegen",
    "deploy:mumbai": "npm run prepare:mumbai && npm run codegen && npm run deploy",
    "prepare:localhost": "npm run build && npx mustache config/localhost.json subgraph.template.yaml subgraph.yaml && npm run copy-abis && npm run codegen",
    "deploy:localhost": "npm run prepare:localhost && npm run create-local && npm run deploy-local",
    "deploy-contract": "hardhat run scripts/deploy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beehive-innovation/rain1155.git"
  },
  "author": "beehive-innovation",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beehive-innovation/rain1155/issues"
  },
  "homepage": "https://github.com/beehive-innovation/rain1155#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.6",
    "eslint": "^8.2.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.9.2",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-storage-layout": "^0.1.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "solc-0.8": "npm:solc@^0.8.10",
    "solhint": "^3.3.4",
    "solidity-docgen": "^0.5.13",
    "ts-generator": "^0.1.1",
    "ts-node": "10.8.1",
    "tslib": "^2.0.0",
    "typechain": "^6.0.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@beehiveinnovation/rain-protocol": "git+https://github.com/beehive-innovation/rain-protocol.git#5a35b097475d37fd5f44446922a32025708c1f8c",
    "@graphprotocol/graph-cli": "0.31.0",
    "@graphprotocol/graph-ts": "0.27.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "dotenv": "^16.0.0",
    "gluegun": "^5.1.2",
    "mustache": "^4.2.0",
    "rain-sdk": "git+https://github.com/beehive-innovation/rain-sdk.git#63f7bafe1baf0425232cf2050aeca7ed37d570ad"
  }
}
